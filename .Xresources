Xft.antialias:                 true
Xft.autohint:                  false
Xft.dpi:                       96
Xft.hinting:                   true
Xft.hintstyle:                 hintslight
Xft.lcdfilter:                 lcddefault
Xft.rgba:                      rgb

! general
*.foreground:                  #c2c8d7
*.background:                  #1c262b
*.cursorcolor:                 #b3b8c3
! black
*.color0:                      #000000
*.color8:                      #777777
! red
*.color1:                      #ee2b2a
*.color9:                      #dc5c60
! green
*.color2:                      #40a33f
*.color10:                     #70be71
! yellow
*.color3:                      #ffea2e
*.color11:                     #fff163
! blue
*.color4:                      #1e80f0
*.color12:                     #54a4f3
! magenta
*.color5:                      #8800a0
*.color13:                     #aa4dbc
! cyan
*.color6:                      #16afca
*.color14:                     #42c7da
! white
*.color7:                      #a4a4a4
*.color15:                     #ffffff
! bold
*.colorBD:                     #ffffff

alock.background*shade:        100
alock.background*blur:         50
alock*frame*input:             #33ff33
alock*frame*check:             #2e9ef4
alock*frame*error:             #ff3333
alock*frame.width:             2

urxvt*depth:                   32
urxvt*buffered:                true
urxvt*geometry:                175x60
urxvt*jumpScroll:              true
urxvt*skipScroll:              true
urxvt*font:                    xft:SauceCodePro Nerd Font:size=10,xft:Source Code Pro:size=10,xft:Inconsolata:size=10,xft:Noto Emoji
urxvt*title:                   urxvt
urxvt*urgentOnBell:            true
urxvt*visualBell:              true
urxvt*scrollstyle:             rxvt
urxvt*thickness:               0
urxvt*scrollBar:               false
urxvt*scrollBar_right:         false
urxvt*scrollBar_floating:      false
urxvt*scrollTtyOutput:         false
urxvt*scrollWithBuffer:        true
urxvt*scrollTtyKeypress:       true
urxvt*saveLines:               10000
urxvt*internalBorder:          0
urxvt*externalBorder:          0
urxvt*borderLess:              false
urxvt*termName:                xterm-256color
urxvt*mouseWheelScrollPage:    false
urxvt*cursorBlink:             true
urxvt*secondaryScreen:         true
urxvt*secondaryScroll:         false
urxvt*secondaryWheel:          true
urxvt*iso14755:                false
urxvt*perl-ext-common:         default,tabbedex,matcher,selection-to-clipboard
urxvt*tabbed.tabbar-fg:        -2
urxvt*tabbed.tabbar-bg:        -1
urxvt*tabbed.tab-fg:           -2
urxvt*tabbed.tab-bg:           4
urxvt*tabbed.bell-fg:          -2
urxvt*tabbed.bell-bg:          9
urxvt*tabbed.bell-tab-fg:      -2
urxvt*tabbed.bell-tab-bg:      9
urxvt*tabbed.new-button:       false
urxvt*tabbed.title:            false
urxvt*tabbed.autohide:         true
urxvt*tabbed.no-tabbedex-keys: true
urxvt*keysym.Control-t:        perl:tabbedex:new_tab
urxvt*keysym.Control-y:        perl:tabbedex:rename_tab
urxvt*keysym.Control-Right:    perl:tabbedex:next_tab
urxvt*keysym.Control-Left:     perl:tabbedex:prev_tab
urxvt*keysym.Shift-Right:      perl:tabbedex:move_tab_right
urxvt*keysym.Shift-Left:       perl:tabbedex:move_tab_left
urxvt*url-launcher:            /usr/bin/firefox
urxvt*matcher.button:          1
urxvt*clipboard.autocopy:      true
urxvt*keysym.Control-v:        eval:paste_clipboard
