{
    "[dockercompose]": {
        "editor.autoIndent": "advanced",
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.insertSpaces": true,
        "editor.quickSuggestions": {
            "comments": false,
            "other": true,
            "strings": true
        },
        "editor.tabSize": 2
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[templ]": {
        "editor.defaultFormatter": "a-h.templ"
    },
    "chat.mcp.autostart": "newAndOutdated",
    "chat.viewSessions.orientation": "stacked",
    "debug.showBreakpointsInOverviewRuler": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "docker.extension.enableComposeLanguageServer": false,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.dragAndDrop": false,
    "editor.fontFamily": "'SauceCodePro Nerd Font', 'Source Code Pro', 'Inconsolata', 'Noto Emoji'",
    "editor.fontSize": 13,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.minimap.showSlider": "always",
    "editor.mouseWheelZoom": true,
    "editor.rulers": [
        120
    ],
    "editor.smoothScrolling": true,
    "editor.stickyScroll.enabled": false,
    "editor.suggest.preview": true,
    "editor.suggestSelection": "first",
    "editor.wordWrapColumn": 120,
    "explorer.incrementalNaming": "smart",
    "extensions.ignoreRecommendations": true,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 5000,
    "files.enableTrash": false,
    "files.hotExit": "off",
    "files.insertFinalNewline": true,
    "files.trimTrailingWhitespace": true,
    "geminicodeassist.inlineSuggestions.enableAuto": false,
    "geminicodeassist.updateChannel": "Insiders",
    "git.alwaysSignOff": true,
    "git.enableSmartCommit": true,
    "git.rebaseWhenSync": true,
    "git.showPushSuccessNotification": true,
    "github.copilot.nextEditSuggestions.enabled": true,
    "gitlens.codeLens.enabled": false,
    "gitlens.showWhatsNewAfterUpgrades": false,
    "go.buildFlags": [
        "-v"
    ],
    "go.formatTool": "goimports",
    "go.survey.prompt": false,
    "go.testFlags": [
        "-v",
        "-race",
        "-count=1"
    ],
    "go.toolsManagement.autoUpdate": false,
    "go.useLanguageServer": true,
    "gopls": {
        "ui.semanticTokens": true
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "problems.showCurrentInStatus": true,
    "python.experiments.enabled": false,
    "redhat.telemetry.enabled": false,
    "rest-client.enableTelemetry": false,
    "rest-client.previewOption": "exchange",
    "rest-client.requestNameAsResponseTabTitle": true,
    "rest-client.showResponseInDifferentTab": true,
    "search.showLineNumbers": true,
    "security.workspace.trust.enabled": false,
    "spellright.documentTypes": [
        "latex",
        "markdown",
        "plaintext",
        "tex",
        "yaml"
    ],
    "spellright.language": [
        "en_US"
    ],
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.copyOnSelection": true,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "window.autoDetectColorScheme": false,
    "window.autoDetectHighContrast": false,
    "window.newWindowDimensions": "maximized",
    "window.openFoldersInNewWindow": "on",
    "workbench.colorTheme": "Iceberg",
    "workbench.commandPalette.preserveInput": true,
    "workbench.editor.empty.hint": "hidden",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.tabSizing": "shrink",
    "workbench.enableExperiments": false,
    "workbench.quickOpen.preserveInput": true,
    "workbench.startupEditor": "none",
    "workbench.tips.enabled": false,
    "xml.format.enforceQuoteStyle": "preferred",
    "yaml.schemaStore.enable": false
}
